#include <iostream>
using namespace std;

int main() {
    int n, num, index = -1;
    cout << "Enter the size of the array: ";
    cin >> n;
    int arr[n];
    cout << "Enter the elements of the array: ";
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }
    cout << "Enter the number to search: ";
    cin >> num;

    for (int i = 0; i < n; i++) {
        if (arr[i] == num) {
            index = i;
            break;
        }
    }
    cout << index << endl;
    return 0;
}
